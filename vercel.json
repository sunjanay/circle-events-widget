{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "CIRCLE_API_KEY": "MSpbmfA3JCzzLjXrQca7yQaLQbkrd9aR",
    "CIRCLE_COMMUNITY_ID": "fostergreatness"
  }
}